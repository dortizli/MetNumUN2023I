    {
      "cell_type": "code",
      "source": [
        "two_day = aapl.resample('2D').mean()\n",
        "three_week = aapl.resample('3W').mean()\n",
        "six_month = aapl.resample('6M').mean()\n",
        "\n",
        "\n",
        "print(two_day)\n",
        "print(three_week)\n",
        "print(six_month )"
      ],
      "metadata": {
        "colab": {
